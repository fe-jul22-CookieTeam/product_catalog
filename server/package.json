{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": "14.16.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "swc src -d dist",
    "build-dev": "npm run build && NODE_ENV=development node --inspect dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "deploy": "git add . && git commit -m Heroku && git push heroku main",
    "postinstall": "tsc"
  },
  "keywords": [
    "heroku"
  ],
  "lint-staged": {
    "*.js": "npm run lint"
  }
}
